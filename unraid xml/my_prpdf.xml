<?xml version="1.0"?>
<Container version="2">
  <Name>PRPDF (beta)</Name>
  <Repository>knex666/prpdf</Repository>
  <Registry/>
  <Network>br0</Network>
  <MyIP>192.168.100.80</MyIP>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
   <Project>https://github.com/maschhoff/prpdf</Project>
  <Overview>This tool is for Preview and Rename scanned PDF Documents. Rulebased autoscan with OCR. Merge etc. 
    Its an open beta that I am testing on. 
    Please add the configuration file from https://raw.githubusercontent.com/maschhoff/prpdf/main/config/config.json to /mnt/user/appdata/prpdf
    Please add a scan source folder 
    and a folder where your files shoud be sorted into the subfolders existing there
    Any Issus report on github.
    Thanks
  </Overview>
  <Category/>
  <WebUI></WebUI>
  <TemplateURL/>
  <Icon>https://cdn2.iconfinder.com/data/icons/adobe-acrobat-pdf/154/adobe-acrobat-pdf-file-document-512.png</Icon>
  <ExtraParams/>
  <PostArgs>python3 /source/prpdf.py</PostArgs>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText>If you like what you see - please buy me a coffee</DonateText>
  <DonateLink>https://www.buymeacoffee.com/maschhoff</DonateLink>
  <Description/>
  <Networking>
    <Mode>br0</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/Archiv/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/source/static/pdf/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/prpdf/</HostDir>
      <ContainerDir>/source/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Files" Target="/Archiv/" Default="" Mode="rw" Description="Archiv Path: /Archiv/" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Path 2" Target="/source/static/pdf/" Default="" Mode="rw" Description="Scan Path: /source/static/pdf/" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Path 4" Target="/source/config" Default="" Mode="rw" Description="Configuration" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/prpdf/</Config>
</Container>
